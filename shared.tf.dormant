resource "aws_instance" "bigiq" {
  count                  = 1
  ami                    = "ami-96d59281" 
  instance_type          = "m4.large"
  subnet_id              = "${aws_subnet.public-e.id}"
  vpc_security_group_ids = ["${aws_security_group.f5_management.id}"]
  key_name               = "${var.aws_keypair}"

  tags {
    Name = "f5-bigiq-5.1"
  }
}